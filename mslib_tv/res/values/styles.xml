<resources>

    <!--
         Base application theme, dependent on API level. This theme is replaced 
		by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
             Theme customizations available in newer API levels can go in res/values-vXX/styles.xml, 
			while customizations related to backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!--
             All customizations that are NOT specific to a particular API-level 
			can go here.
        -->
    </style>

    <declare-styleable name="WebImageView">
        <attr name="network_animation" format="reference" />
        <attr name="conerRadius" format="dimension" />
        <attr name="category">
            <enum name="raw" value="0" />
            <enum name="thumbnail" value="1" />
            <enum name="small_200" value="2" />
        </attr>
        <attr name="optim_overdraw" format="boolean" />
        <attr name="default_src" format="reference" />
    </declare-styleable>

</resources>